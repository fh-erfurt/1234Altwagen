@startuml
class Location {
    -address: Address
    -name: string
    -maxCars: int
    -carCount: int
    +Location(Address, String, int)
    {method} getter und setter
    +addCar(Car): boolean
    +removeCar(Car): boolean
    +equals(Object): boolean
    -decreaseCarCount(): boolean
    -increaseCarCount(): boolean
}
@enduml